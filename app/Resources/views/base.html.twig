<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8"/>
    <title>{% block title %}DEA Courriers {% endblock %}</title>
    {% block stylesheets %}
        <link href='http://fonts.googleapis.com/css?family=Oswald:400,300,700' rel='stylesheet' type='text/css'>
        <link href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700&subset=all" rel="stylesheet"
              type="text/css"/>
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
        <link href="{{ asset('bundles/courriers/plugins/simple-line-icons/simple-line-icons.min.css') }}"
              rel="stylesheet" type="text/css"/>
        <link href="{{ asset('bundles/courriers/plugins/bootstrap/css/bootstrap.min.css') }}" rel="stylesheet"
              type="text/css"/>
        <link href="{{ asset('bundles/courriers/plugins/uniform/css/uniform.default.css') }}" rel="stylesheet"
              type="text/css"/>
        <link rel="stylesheet" type="text/css" href="{{ asset('bundles/courriers/plugins/select2/select2.css') }}"/>
        <link rel="stylesheet" type="text/css"
              href="{{ asset('bundles/courriers/plugins/datatables/plugins/bootstrap/dataTables.bootstrap.css') }}"/>
        <link rel="stylesheet"
              href="{{ asset('bundles/courriers/plugins/bootstrap-datepicker/css/bootstrap-datepicker3.min.css') }}">
        <link href="{{ asset('bundles/courriers/css/components-rounded.css') }}" id="style_components" rel="stylesheet"
              type="text/css"/>
        <link href="{{ asset('bundles/courriers/css/plugins.css') }}" rel="stylesheet" type="text/css"/>
        <link href="{{ asset('bundles/courriers/css/layout.css') }}" rel="stylesheet" type="text/css"/>
        <link href="{{ asset('bundles/courriers/css/custom.css') }}" rel="stylesheet" type="text/css"/>
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
</head>
<body class="page-header-fixed page-quick-sidebar-over-content">
<div class="wrapper">
    <!-- Header BEGIN -->
    <header class="page-header">
        <nav class="navbar mega-menu" role="navigation">
            <div class="container-fluid">
                <div class="clearfix navbar-fixed-top">
                    <!-- Brand and toggle get grouped for better mobile display -->
                    <button type="button" class="navbar-toggle" data-toggle="collapse"
                            data-target=".navbar-responsive-collapse">
                        <span class="sr-only">Toggle navigation</span>
		                    <span class="toggle-icon">
		                        <span class="icon-bar"></span>
		                        <span class="icon-bar"></span>
		                        <span class="icon-bar"></span>
		                    </span>
                    </button>
                    <!-- End Toggle Button -->

                    <!-- BEGIN LOGO -->
                    <a id="index" class="page-logo" href="index.html">
                        <img src="{{ asset('bundles/courriers/img/logo.png') }}" alt="Logo">
                    </a>
                    <!-- END LOGO -->

                    <!-- BEGIN TOPBAR ACTIONS -->
                    <div class="topbar-actions">
                        <!-- BEGIN USER PROFILE -->
                        <div class="btn-group-img btn-group">
                            <a href="{{ path('fos_user_security_logout') }}" class="btn btn-default"><span
                                        class="icon icon-logout"></span></a>
                        </div>
                        <!-- END USER PROFILE -->
                    </div>
                    <!-- END TOPBAR ACTIONS -->
                </div>

                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="nav-collapse collapse navbar-collapse navbar-responsive-collapse">
                    <ul class="nav navbar-nav text-uppercase">
                        <li class="dropdown dropdown-fw open selected">
                            <a href="javascript:;">
                                Tableau de Bord
                            </a>
                            <ul class="dropdown-menu dropdown-menu-fw">
                                <li><a href="Javascript:void(0)" id="ajaxentree">Courriers en entrée</a>
                                </li>
                                <li><a href="Javascript:void(0)" id="ajaxtraite">Courriers à traiter</a></li>
                                <li><a href="Javascript:void(0)" id="ajaxvalide">Courriers à valider</a></li>
                                <li><a href="Javascript:void(0)" id="ajaxretard">Courriers en retard</a></li>
                                <li><a href="Javascript:void(0)" id="ajaxmescourriers">Mes courriers</a></li>
                                <li><a href="Javascript:void(0)" id="ajaxarchive">Courriers archivés</a></li>
                            </ul>
                        </li>
                        <li class="dropdown dropdown-fw selected">
                            <a href="javascript:;">
                                Courriers
                            </a>
                            <ul class="dropdown-menu dropdown-menu-fw">
                                <li><a href="Javascript:void(0)" id="ajaxnvdepart">Nv. courrier de départ</a></li>
                                <li><a href="Javascript:void(0)" id="ajaxnvarrivee">Nv. courrier d'arrivée</a></li>
                            </ul>
                        </li>
                        <li class="dropdown dropdown-fw selected">
                            <a href="javascript:;">
                                Affectations
                            </a>
                            <ul class="dropdown-menu dropdown-menu-fw">
                                <li><a href="inner.html">Nv. Affectation de départ</a></li>
                                <li><a href="inner.html">Nv. Affectation d'arrivée</a></li>
                            </ul>
                        </li>
                        <li class="dropdown dropdown-fw selected">
                            <a href="javascript:;">
                                Administration
                            </a>
                            <ul class="dropdown-menu dropdown-menu-fw">
                                <li><a href="Javascript:void(0)" id="ajaxdivisions">Divisions</a></li>
                                <li><a href="Javascript:void(0)" id="ajaxservices">Services</a></li>
                                <li><a href="Javascript:void(0)" id="ajaxusers">Utilisateurs</a></li>
                            </ul>
                        </li>
                        <li class="dropdown dropdown-fw selected">
                            <a href="javascript:;">
                                Profil
                            </a>
                            <ul class="dropdown-menu dropdown-menu-fw">
                                <li><a href="inner.html">Mes Informations</a></li>
                                <li><a href="inner.html">Historique des opérations</a></li>
                            </ul>
                        </li>
                    </ul>
                </div>
                <!-- END NAVBAR COLLAPSE -->
            </div>
            <!--/container-->
        </nav>
    </header>
    <!-- Header END -->

    <!-- Page Content BEGIN -->
    <div class="container-fluid">
        <div class="page-content">
            <div class="row">
                <div class="col-md-12">
                    <center>
                        <div id="loading"></div>
                    </center>
                    <!-- BEGIN Portlet PORTLET-->
                    <div class="portlet light bordered" id="main">
                        <div class="portlet-body form">
                            <div id="ajaxcontent">

                            </div>
                            <div style="display: none;">
                                {% block body %}{% endblock %}
                            </div>
                        </div>
                    </div>
                    <!-- END PORTLET-->
                </div>
            </div>
        </div>
        <!-- Page Content END -->

        <!-- Copyright BEGIN -->
        <p class="copyright">2015 © DEA - Ministère de l'Intérieur.</p>
        <!-- Copyright END -->
    </div>
</div>
<a href="#index" class="go2top"><i class="icon-arrow-up"></i></a>
